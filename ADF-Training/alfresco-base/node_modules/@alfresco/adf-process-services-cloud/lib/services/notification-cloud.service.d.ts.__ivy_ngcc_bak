import { Apollo } from 'apollo-angular';
import { HttpLink } from 'apollo-angular/http';
import { AppConfigService, AlfrescoApiService } from '@alfresco/adf-core';
import { BaseCloudService } from './base-cloud.service';
export declare class NotificationCloudService extends BaseCloudService {
    apollo: Apollo;
    private http;
    appsListening: any[];
    constructor(apiService: AlfrescoApiService, appConfigService: AppConfigService, apollo: Apollo, http: HttpLink);
    private get webSocketHost();
    private get protocol();
    initNotificationsForApp(appName: string): void;
    makeGQLQuery(appName: string, gqlQuery: string): import("rxjs").Observable<import("@apollo/client/core").FetchResult<unknown, Record<string, any>, Record<string, any>>>;
}
