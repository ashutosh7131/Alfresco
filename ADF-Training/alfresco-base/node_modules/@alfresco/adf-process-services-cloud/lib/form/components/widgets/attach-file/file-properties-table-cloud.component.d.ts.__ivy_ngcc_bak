import { EventEmitter } from '@angular/core';
import { LocalizedDatePipe, ThumbnailService } from '@alfresco/adf-core';
import { Node } from '@alfresco/js-api';
export declare class FilePropertiesTableCloudComponent {
    private localizedDatePipe;
    private thumbnailService;
    static RETRIEVE_METADATA_OPTION: string;
    uploadedFiles: any;
    hasFile: boolean;
    selectedNode: Node;
    field: any;
    displayedColumns: any;
    mimeTypeIcon: any;
    rowClick: EventEmitter<Node>;
    attachFileClick: EventEmitter<any>;
    downloadFile: EventEmitter<Node>;
    contentModelFileHandler: EventEmitter<any>;
    removeAttachFile: EventEmitter<any>;
    constructor(localizedDatePipe: LocalizedDatePipe, thumbnailService: ThumbnailService);
    onRowClicked(file?: Node): void;
    onAttachFileClicked(nodeSelector: any): void;
    downloadContent(file: Node): void;
    contentModelFormFileHandler(file?: any): void;
    onRemoveAttachFile(file: any): void;
    getIcon(mimeType: string): string;
    getColumnValue(file: any, displayableCMProperty: any): string;
    checkDateTypeAndTransform(dataType: any, value: any): string;
    displayMenuOption(option: string): boolean;
}
