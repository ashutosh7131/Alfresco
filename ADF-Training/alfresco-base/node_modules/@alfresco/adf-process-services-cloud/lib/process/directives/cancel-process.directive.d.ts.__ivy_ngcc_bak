import { EventEmitter, OnInit, OnDestroy, ElementRef } from '@angular/core';
import { IdentityUserService } from '@alfresco/adf-core';
import { ProcessCloudService } from '../services/process-cloud.service';
import { ProcessInstanceCloud } from '../start-process/models/process-instance-cloud.model';
export declare class CancelProcessDirective implements OnInit, OnDestroy {
    private elementRef;
    private processCloudService;
    private identityUserService;
    success: EventEmitter<any>;
    error: EventEmitter<any>;
    processInstanceDetails: ProcessInstanceCloud;
    canCancelProcess: boolean;
    private onDestroy$;
    constructor(elementRef: ElementRef, processCloudService: ProcessCloudService, identityUserService: IdentityUserService);
    ngOnInit(): void;
    onClick(): void;
    private setElementVisibility;
    checkCanCancelProcess(): boolean;
    cancelProcess(): void;
    ngOnDestroy(): void;
}
