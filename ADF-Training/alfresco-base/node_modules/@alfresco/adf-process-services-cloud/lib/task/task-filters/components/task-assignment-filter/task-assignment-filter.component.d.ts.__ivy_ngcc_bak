import { EventEmitter, OnInit } from '@angular/core';
import { FormControl } from '@angular/forms';
import { IdentityGroupModel, IdentityUserModel, IdentityUserService } from '@alfresco/adf-core';
import { AssignmentType, TaskFilterProperties } from '../../models/filter-cloud.model';
export declare class TaskAssignmentFilterCloudComponent implements OnInit {
    private identityUserService;
    appName: string;
    taskFilterProperty: TaskFilterProperties;
    assignedChange: EventEmitter<IdentityUserModel>;
    assignedGroupChange: EventEmitter<IdentityGroupModel[]>;
    assignmentType: AssignmentType;
    candidateGroups: IdentityGroupModel[];
    groupForm: FormControl;
    assignmentTypeList: {
        unassigned: AssignmentType;
        currentUser: AssignmentType;
        candidateGroups: AssignmentType;
    };
    constructor(identityUserService: IdentityUserService);
    ngOnInit(): void;
    isCandidateGroupsType(): boolean;
    onAssignmentTypeChange(type: any): void;
    onChangedGroups(groups: IdentityGroupModel[]): void;
    private setDefaultAssignmentType;
    private setDefaultAssignedGroups;
    private isFilterPropertyDefined;
}
