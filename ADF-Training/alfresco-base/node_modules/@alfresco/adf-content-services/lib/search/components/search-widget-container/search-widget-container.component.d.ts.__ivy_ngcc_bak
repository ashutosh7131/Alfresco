import { ViewContainerRef, OnInit, OnDestroy, ComponentRef, ComponentFactoryResolver, SimpleChanges, OnChanges } from '@angular/core';
import { SearchFilterService } from '../../services/search-filter.service';
import { BaseQueryBuilderService } from '../../services/base-query-builder.service';
import { Observable } from 'rxjs';
export declare class SearchWidgetContainerComponent implements OnInit, OnDestroy, OnChanges {
    private searchFilterService;
    private queryBuilder;
    private componentFactoryResolver;
    content: ViewContainerRef;
    id: string;
    selector: string;
    settings: any;
    config: any;
    value: any;
    componentRef: ComponentRef<any>;
    constructor(searchFilterService: SearchFilterService, queryBuilder: BaseQueryBuilderService, componentFactoryResolver: ComponentFactoryResolver);
    ngOnInit(): void;
    ngOnChanges(changes: SimpleChanges): void;
    private setupWidget;
    ngOnDestroy(): void;
    applyInnerWidget(): void;
    setValue(currentValue: string | Object): void;
    hasValueSelected(): any;
    getCurrentValue(): any;
    getDisplayValue(): Observable<string> | null;
    resetInnerWidget(): void;
}
