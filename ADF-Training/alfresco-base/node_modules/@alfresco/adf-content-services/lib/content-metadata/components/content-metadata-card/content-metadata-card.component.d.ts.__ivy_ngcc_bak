import { OnChanges, SimpleChanges } from '@angular/core';
import { Node } from '@alfresco/js-api';
import { ContentService, VersionCompatibilityService } from '@alfresco/adf-core';
import { NodeAspectService } from '../../../aspect-list/node-aspect.service';
import { PresetConfig } from '../../interfaces/content-metadata.interfaces';
export declare class ContentMetadataCardComponent implements OnChanges {
    private contentService;
    private nodeAspectService;
    private versionCompatibilityService;
    node: Node;
    displayEmpty: boolean;
    displayAspect: string;
    preset: string | PresetConfig;
    readOnly: boolean;
    multi: boolean;
    private _displayDefaultProperties;
    set displayDefaultProperties(value: boolean);
    get displayDefaultProperties(): boolean;
    editable: boolean;
    expanded: boolean;
    editAspectSupported: boolean;
    constructor(contentService: ContentService, nodeAspectService: NodeAspectService, versionCompatibilityService: VersionCompatibilityService);
    ngOnChanges(changes: SimpleChanges): void;
    onDisplayDefaultPropertiesChange(): void;
    toggleEdit(): void;
    toggleExpanded(): void;
    hasAllowableOperations(): boolean;
    openAspectDialog(): void;
    isEditAspectSupported(): boolean;
}
