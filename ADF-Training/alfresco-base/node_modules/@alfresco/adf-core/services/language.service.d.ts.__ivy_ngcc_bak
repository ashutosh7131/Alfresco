import { Direction } from '@angular/cdk/bidi';
import { AppConfigService } from '../app-config/app-config.service';
import { UserPreferencesService } from './user-preferences.service';
export interface LanguageItem {
    key: string;
    label: string;
    direction?: Direction;
}
export declare class LanguageService {
    private userPreferencesService;
    private languages;
    languages$: import("rxjs").Observable<LanguageItem[]>;
    constructor(appConfigService: AppConfigService, userPreferencesService: UserPreferencesService);
    changeLanguage(language: LanguageItem): void;
    setLanguages(items: LanguageItem[]): void;
}
