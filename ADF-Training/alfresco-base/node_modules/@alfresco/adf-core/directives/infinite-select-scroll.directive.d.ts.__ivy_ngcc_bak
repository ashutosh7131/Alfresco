import { AfterViewInit, EventEmitter, OnDestroy } from '@angular/core';
import { MatSelect } from '@angular/material/select';
export declare class InfiniteSelectScrollDirective implements AfterViewInit, OnDestroy {
    private matSelect;
    static readonly MAX_ITEMS = 50;
    scrollEnd: EventEmitter<Event>;
    private onDestroy$;
    private itemHeightToWaitBeforeLoadNext;
    constructor(matSelect: MatSelect);
    ngAfterViewInit(): void;
    ngOnDestroy(): void;
    private handleScrollEvent;
    private isScrollInNextFetchArea;
    private getItemHeight;
}
