import { EventEmitter, Injectable } from '@angular/core';
import { of } from 'rxjs';
export class TranslationMock {
    constructor() {
        this.defaultLang = 'en';
        this.translate = {
            onLangChange: new EventEmitter()
        };
    }
    addTranslationFolder() { }
    onTranslationChanged() { }
    use() { }
    loadTranslation() { }
    get(key) {
        return of(key);
    }
    instant(key) {
        return key;
    }
}
TranslationMock.decorators = [
    { type: Injectable }
];
