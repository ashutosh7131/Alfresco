import { Pipe } from '@angular/core';
import moment from 'moment-es6';
export class MomentDatePipe {
    transform(value, dateFormat) {
        return moment(value, dateFormat);
    }
}
MomentDatePipe.decorators = [
    { type: Pipe, args: [{ name: 'adfMomentDate' },] }
];
