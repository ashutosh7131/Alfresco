                        formFieldModel.push.apply(formFieldModel, __spread(column.fields));
                    });
                }
            }
            return formFieldModel;
        };
        FormModel.prototype.markAsInvalid = function () {
            this.isValid = false;
        };
        FormModel.prototype.parseOutcomes = function () {
