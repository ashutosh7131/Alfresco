            this.editable = false;
            this.title = 'JSON';
            this.value = '';
        }
        EditJsonDialogComponent.prototype.ngOnInit = function () {
            if (this.settings) {
                this.editable = this.settings.editable;
                this.value = this.settings.value || '';
                this.title = this.settings.title || 'JSON';
            }
        };
        return EditJsonDialogComponent;
    }());
