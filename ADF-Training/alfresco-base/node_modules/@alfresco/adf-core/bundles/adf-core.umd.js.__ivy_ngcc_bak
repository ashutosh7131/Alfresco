                    this._externalViewer = this.viewerExtensions.find(function (ext) { return ext.fileExtension === '*'; });
                }
                return this._externalViewer;
            },
            enumerable: false,
            configurable: true
        });
        Object.defineProperty(ViewerComponent.prototype, "sharedLinksApi", {
