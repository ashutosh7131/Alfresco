    var TranslationService = /** @class */ (function () {
        function TranslationService(translate, userPreferencesService, providers) {
            var e_1, _a;
            var _this = this;
            this.translate = translate;
            this.customLoader = this.translate.currentLoader;
            this.defaultLang = 'en';
