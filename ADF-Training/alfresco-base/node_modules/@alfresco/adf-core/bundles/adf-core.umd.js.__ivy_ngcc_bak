            var optionalParams = [];
            for (var _i = 1; _i < arguments.length; _i++) {
                optionalParams[_i - 1] = arguments[_i];
            }
            if (this.currentLogLevel >= LogLevelsEnum.TRACE) {
                this.messageBus(message, 'LOG');
                console.log.apply(console, __spread([message], optionalParams));
            }
