import { PipeTransform } from '@angular/core';
import { LocalizedDatePipe } from '@alfresco/adf-core';
import { ProcessInstance } from '../process-list';
export declare class ProcessNamePipe implements PipeTransform {
    private localizedDatePipe;
    static DATE_TIME_IDENTIFIER_REG_EXP: RegExp;
    static PROCESS_DEFINITION_IDENTIFIER_REG_EXP: RegExp;
    constructor(localizedDatePipe: LocalizedDatePipe);
    transform(processNameFormat: string, processInstance?: ProcessInstance): string;
}
